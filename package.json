{
  "dependencies": {
    "@gctools-components/eslint-config": "^1.1.3",
    "bootstrap": "^4.1.1",
    "gatsby": "2.0.0-beta.59",
    "gatsby-image": "next",
    "gatsby-plugin-eslint": "^1.0.3",
    "gatsby-plugin-lunr": "^1.0.0",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-remark-images": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "i18next": "^11.6.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "node-sass": "^4.9.0",
    "popper.js": "^1.14.3",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-prism": "^4.3.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-i18next": "^7.10.1",
    "reactstrap": "^6.1.0",
    "rehype-react": "^3.0.2"
  },
  "license": "MIT",
  "scripts": {
    "clear": "rm -rf .cache/*",
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write 'src/**/*.js'",
    "test": "yarn run build; jest; eslint src/",
    "lint-fix": "eslint src/ --fix",
    "deploy": "rm -rf .cache/* && gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "loader-utils": "^1.1.0",
    "prettier": "^1.12.0",
    "react-test-renderer": "^16.4.2"
  },
  "eslintConfig": {
    "extends": "@gctools-components",
    "root": true
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": ["node_modules", ".cache"],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost:8000/",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ]
  }
}
